.pl-calendar{
  display: flex;
  flex-direction: column;
}
.pl-calendar .calendar-header {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.pl-calendar .calendar-header .title {
  
  font-size: 36rpx;
  font-family: Source Han Sans CN;
  font-weight: 400;
  color: #DEA360;
}
.pl-calendar .calendar-header .btn {
  position: relative;
  width: 50rpx;
  height: 50rpx;
  background-image: url(data:image/png;base64,);
  background-color: transparent;
  background-repeat: no-repeat;
  background-size: 13rpx 23rpx;
  background-position: center center;
}
.pl-calendar .calendar-header .btn.btn-next {
  background-image: url(data:image/png;base64,);
}
.pl-calendar .calendar-body{
  display: flex;
  flex-direction: column;
}
.pl-calendar .week-label-box {
  margin-top: 40rpx;
  padding: 10rpx 0;
}
.pl-calendar .table-row {
  display: table;
  table-layout: fixed;
}
.pl-calendar .table-col {
  display: table-cell;
  width: 80rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
}
.pl-calendar .day {
  position: relative;
  color: #525252;
  border-radius: 50%;
  justify-content:center;
  width:60rpx;
  height:60rpx;
  line-height:60rpx;
  margin:0px auto;
}
.pl-calendar .is-weekend {
  color: #EB4F04;
}
.pl-calendar .before-today {
  color: #939393;
}
.pl-calendar .mark {
  color: #fff;
  background-image: url(data:image/png;base64,);
  background-position: center;
  background-repeat: no-repeat;
  background-size: 71rpx 71rpx;
}
.pl-calendar .is-today{
  color: #F67A53;
}
.pl-calendar .is-today:after {
  content: '';
  display: block;
  width: 10rpx;
  height: 10rpx;
  margin: 8rpx auto 0;
  border-radius: 50%;
  background: #F67A53;
}