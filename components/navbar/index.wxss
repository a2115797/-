@import '/app.wxss';

.navbar {
  /* position: fixed;
  background-color: #fff;
  top: 0;
  left: 0;
  z-index: 999;
  width: 100%; */
  height:136rpx;
}

.navbar.on .navbarH{
  background-color: #e93323;
}

.navbar.black .navbarH{
  background-color: #232323;
  background-color: #000;
}

.navbar.gray .navbarH{
  background-color: #666;
}

.navbar.gray01 .navbarH{
  background-color: #efefef;
}

.navbar.red .navbarH{
  background-color: #eb5447;
}

.navbar.gradual01 .navbarH{
  background-image: linear-gradient(to right, #f67837 0%, #f11d09 100%);
}

.navbar .navbarH {
  /* position: relative; */
  position: fixed;
  background-color: #fff;
  top: 0;
  left: 0;
  z-index: 999;
  width: 100%;
}

.navbar .navbarH .navbarCon {
  position: absolute;
  bottom: 0;
  display:flex;
  flex-wrap:wrap;
  align-items:center;
  justify-content:center;
  height: 100rpx;
  width: 100%;
}

.navbar .navbarH .navbarCon .name {
  width: 300rpx;
  text-align: center;
  font-size: 30rpx;
  font-family: Source Han Sans CN;
  font-weight: 400;
  color: #33342E;
  
}

.navbar .navbarH .navbarCon .name.on {
  color: #fff;
}

.navbar .navbarH .navbarCon .icon-l {
  color: #000;
  font-weight: bold;
  position: absolute;
  font-size: 40rpx;
  left: 16rpx;
  width: 100rpx;
  height: 56rpx;
  line-height: 54rpx;
  top: 50%;
  transform: translateY(-50%);
}

.navbar .navbarH .navbarCon .icon-l.on {
  color: #fff;
}

.navbar .navbarH .navbarCon .logo {
  width: 127rpx;
  height: 45rpx;
  position: absolute;
  left: 30rpx;
  top: 50%;
  transform: translateY(-50%);
}

.navbar .navbarH .navbarCon .logo image {
  width: 100%;
  height: 100%;
  display: block;
}

.navbar .navbarH .navbarCon .search {
  display:flex;
  align-items:center;
  width: 360rpx;
  height: 64rpx;
  background-color: #f7f7f7;
  border-radius: 50rpx;
  padding: 0 28rpx;
  box-sizing: border-box;
  font-size: 28rpx;
  color: #bbb;
  margin-right: 26rpx;
}

.navbar .navbarH .navbarCon .search .icon-xiazai5 {
  font-size: 34rpx;
  margin-right: 16rpx;
}
